{
	"swagger": "2.0",
	"info": {
	  "version": "1.0.0",
	  "title": "API REST pour Swagger"
	},
	"basePath": "/",
	"schemes": ["http"],
	"JWT": {
		"type": "apiKey",
		"name": "Authorization",
		"in": "header"
	  }
	,
	"paths": {
		"/user/me": {
			"get": {
				"summary": "Récupérer le détail d'un user",
				"tags": ["User"],
				"produces": ["application/json"],
				"parameters": [{
					"name": "id",
					"in": "path",
					"required": true,
					"type": "string",
					"description": "Identifiant de l'utilisateur"
					}],
				"responses": {
					"200": {
						"description": "détail d'un utilisateur",
						"schema": {
							"type": "objet"
						},
		
						"properties": {
							"id": {
								"type": "integer",
								"description": "Identifiant de l'utilisateur"
							},
							"name": {
								"type": "string",
								"description": "Nom de l'utilisateur"
							},
							"email": {
								"type": "string",
								"description": "Adresse e-mail de l'utilisateur"
							}
						}
					}
				}
			}
		}
	}

	
}
  